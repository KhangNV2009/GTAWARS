CMD:setstat(playerid, params[])
{
	//if(PlayerInfo[playerid][pAdmin] >= 5)
	//{
		new giveplayerid, statcode, amount, setstatS[128];
		if(sscanf(params, "udd", giveplayerid, statcode, amount))
		{
			SendClientMessageEx(playerid, COLOR_GREY, "SU DUNG: /setstat [player] [statcode] [amount]");
			SendClientMessageEx(playerid, COLOR_GRAD4, "|1 Scores |2 Kills |3 deaths |4 Money |5 Helper |6 Admin |7 Coin |8 ConnectHours");
			return 1;
		}
		if(IsPlayerConnected(giveplayerid))
		{
			switch (statcode)
			{
				case 1:
				{
					SetPlayerScore(giveplayerid, amount);
					format(setstatS, sizeof(setstatS), "   %s's Scores has been set to %d.", PlayerName(giveplayerid), amount);
				}
				case 2:
				{
					PlayerInfo[giveplayerid][pKills] = amount;
					format(setstatS, sizeof(setstatS), "   %s's Kills has been set to %d.", PlayerName(giveplayerid), amount);
				}
				case 3:
				{
					PlayerInfo[giveplayerid][pDeaths] = amount;
					format(setstatS, sizeof(setstatS), "   %s's Deaths has been set to %d.", PlayerName(giveplayerid), amount);
				}
				case 4:
				{
					ResetPlayerMoney(giveplayerid);
					GivePlayerMoney(giveplayerid, amount);
					format(setstatS, sizeof(setstatS), "   %s's Money has been set to %d.", PlayerName(giveplayerid), amount);
				}
				case 5:
				{
					PlayerInfo[giveplayerid][pHelper] = amount;
					format(setstatS, sizeof(setstatS), "   %s's Helper has been set to %d.", PlayerName(giveplayerid), amount);
					foreach(new i: Player)
					{
  		 				if(PlayerInfo[i][pAdmin] >= 1)
			    		{
							SendClientMessageEx(giveplayerid, COLOR_YELLOW, setstatS);
							SendClientMessageEx(i, COLOR_YELLOW, setstatS);
						}
					}
				}
				case 6:
				{
					PlayerInfo[giveplayerid][pAdmin] = amount;
					format(setstatS, sizeof(setstatS), "   %s's Admin has been set to %d.", PlayerName(giveplayerid), amount);
					foreach(new i: Player)
					{
  		 				if(PlayerInfo[i][pAdmin] >= 1)
			    		{
							SendClientMessageEx(giveplayerid, COLOR_YELLOW, setstatS);
							SendClientMessageEx(i, COLOR_YELLOW, setstatS);
						}
					}
				}
				case 7:
				{
					PlayerInfo[giveplayerid][pCoin] = amount;
					format(setstatS, sizeof(setstatS), "   %s's Coin has been set to %d.", PlayerName(giveplayerid), amount);
				}
				case 8:
				{
					PlayerInfo[giveplayerid][pConnectHours] = amount;
					format(setstatS, sizeof(setstatS), "   %s's ConnectHours has been set to %d.", PlayerName(giveplayerid), amount);
				}
				default:
				{
					format(setstatS, sizeof(setstatS), "   Invalid stat code.", amount);
				}
			}
			format(setstatS, sizeof(setstatS), "%s by %s", setstatS, PlayerName(playerid));
			Log("logs/setstats.log", setstatS);
			SendClientMessageEx(playerid, COLOR_GRAD1, setstatS);
		}
	//}
	return 1;
}

CMD:givestat(playerid, params[])
{
	if(PlayerInfo[playerid][pAdmin] >= 5)
	{
		new giveplayerid, statcode, amount, setstatS[128];
		if(sscanf(params, "udd", giveplayerid, statcode, amount))
		{
			SendClientMessageEx(playerid, COLOR_GREY, "SU DUNG: /givestat [player] [statcode] [amount]");
			SendClientMessageEx(playerid, COLOR_GRAD4, "|1 Scores |2 Kills |3 deaths |4 Money |5 Coin |6 ConnectHours");
			return 1;
		}
		if(IsPlayerConnected(giveplayerid))
		{
			switch (statcode)
			{
				case 1:
				{
					SetPlayerScore(giveplayerid, GetPlayerScore(giveplayerid)+amount);
					format(setstatS, sizeof(setstatS), "   %s's Scores has been give to %d.", PlayerName(giveplayerid), amount);
				}
				case 2:
				{
					PlayerInfo[giveplayerid][pKills] += amount;
					format(setstatS, sizeof(setstatS), "   %s's Kills has been give to %d.", PlayerName(giveplayerid), amount);
				}
				case 3:
				{
					PlayerInfo[giveplayerid][pDeaths] += amount;
					format(setstatS, sizeof(setstatS), "   %s's Deaths has been give to %d.", PlayerName(giveplayerid), amount);
				}
				case 4:
				{
					GivePlayerMoney(giveplayerid, amount);
					format(setstatS, sizeof(setstatS), "   %s's Money has been give to %d.", PlayerName(giveplayerid), amount);
				}
				case 5:
				{
					PlayerInfo[giveplayerid][pCoin] += amount;
					format(setstatS, sizeof(setstatS), "   %s's Coin has been give to %d.", PlayerName(giveplayerid), amount);
				}
				case 6:
				{
					PlayerInfo[giveplayerid][pConnectHours] += amount;
					format(setstatS, sizeof(setstatS), "   %s's ConnectHours has been give to %d.", PlayerName(giveplayerid), amount);
				}
				default:
				{
					format(setstatS, sizeof(setstatS), "   Invalid stat code.", amount);
				}
			}
			format(setstatS, sizeof(setstatS), "%s by %s", setstatS, PlayerName(playerid));
			Log("logs/givestats.log", setstatS);
			SendClientMessageEx(playerid, COLOR_GRAD1, setstatS);
		}
	}
	return 1;
}

