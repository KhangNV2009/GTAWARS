		 MedicWeapons(playerid);
    }
    else if(gClass[playerid] == Supporter)
	{
		 SupporterWeapons(playerid);
    }
    else if(gClass[playerid] == Spy)
	{
		 SpyWeapons(playerid);
    }
    else if(gClass[playerid] == Pyroman)
	{
		 PyromanWeapons(playerid);
    }
    else if(gClass[playerid] == Engineer)
	{
		 EngineerWeapons(playerid);
    }
    else if(gClass[playerid] == Pilot)
	{
		 PilotWeapons(playerid);
    }
    else if(gClass[playerid] == Donator)
	{
		 DonatorWeapons(playerid);
    }
    if(gTeam[playerid] == Guerilla)
