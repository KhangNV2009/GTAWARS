CMD:playstream(playerid, params[])
{
	if(PlayerInfo[playerid][pAdmin] < 5) return SendClientMessageEx(playerid, COLOR_RED, "You do not have permission to use this command!");
	if(isnull(params))
	{
		ShowPlayerDialog(playerid,DIALOG_ARADIO, DIALOG_STYLE_INPUT, "Radio", "Enter the exact url of the song you want to play. (.mp3 or .ogg or .vorbis)", "Enter", "Cancel");
		SendClientMessageEx(playerid, COLOR_RED, "If you want to stream music to a specific player please type: /playstream [ID] [Link]");
		return 1;
	}
	if(!isnull(params))
	{
		new target, link[256];
		if(sscanf(params, "us[256]", target, link)) return SendClientMessageEx(playerid, COLOR_ORANGE, "[*] Usage: /playstream [playerid/name] [link]");
		if(target == INVALID_PLAYER_ID) return SendClientMessageEx(playerid, COLOR_ORANGE, "[*] Enter a valid playerid ID/name!");
		PlayAudioStreamForPlayer(target, link);
		format(gString, SOS, "Admin %s bat dau share nhac cho moi nguoi trong server", PlayerName(playerid));
		SendClientMessageEx(target,-1, gString);
		format(gString, SOS, "You have streamed the link: %s", link);
		SendClientMessageEx(playerid, green, gString);
		format(gString, SOS, "For %s[%d].", PlayerName(target), target);
		SendClientMessageEx(playerid, green, gString);
	}

    return 1;
}
CMD:setallskin(playerid,params[])
