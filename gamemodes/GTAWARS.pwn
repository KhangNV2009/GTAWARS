CMD:getid(playerid,params[]) {
	if(isnull(params)) return SendClientMessageEx(playerid,blue,"Correct Usage: /getid [part of nick]");
	if(strlen(params) < 3) return SendClientMessageEx(playerid, COLOR_RED, "You must enter more then 3 characters!");
	new found, string[128], playername[MAX_PLAYER_NAME];
	format(string,sizeof(string),"Searched for: \"%s\" ",params);
	SendClientMessageEx(playerid,blue,string);
	foreach(Player,i)
	{
		if(IsPlayerConnected(i))
		{
	  		GetPlayerName(i, playername, MAX_PLAYER_NAME);
			new namelen = strlen(playername);
			new bool:searched=false;
	    	for(new pos=0; pos <= namelen; pos++)
			{
				if(searched != true)
				{
					if(strfind(playername,params,true) == pos)
					{
		                found++;
						format(string,sizeof(string),"%d. %s (ID %d)",found,playername,i);
						SendClientMessageEx(playerid, -1 ,string);
						searched = true;
					}
				}
			}
		}
	}
	if(found == 0) SendClientMessageEx(playerid, COLOR_RED, "No players have this in their nick");
	return 1;
}

CMD:ip(playerid, params[])
{
	new ip[16];
    if(PlayerInfo[playerid][pAdmin] >= 1)
    {
	    new target;
		if(sscanf(params, "u", target)) return SendClientMessageEx(playerid, COLOR_ORANGE, "[*] Usage: /ip [playerid/name]");
		else if(target == INVALID_PLAYER_ID) return SendClientMessageEx(playerid, COLOR_ORANGE, "[*] Enter a valid player ID/name!");
		GetPlayerIp(target, ip, sizeof(ip));
		format(gString, sizeof(gString), "|- %s[%d]'s IP: %s -|", PlayerName(target), target, ip);
		SendClientMessageEx(playerid, COLOR_GREEN, gString);
		format(gString, sizeof(gString), "[*] Admin %s[%d] | Level: %d | Command: IP", PlayerName(playerid), playerid, PlayerInfo[playerid][pAdmin]);
	}
	else if(PlayerInfo[playerid][pHelper] >= 2)
	{
	    new target;
		if(sscanf(params, "u", target)) return SendClientMessageEx(playerid, COLOR_ORANGE, "[*] Usage: /ip [playerid/name]");
		else if(target == INVALID_PLAYER_ID) return SendClientMessageEx(playerid, COLOR_ORANGE, "[*] Enter a valid player ID/name!");
		GetPlayerIp(target, ip, sizeof(ip));
		format(gString, sizeof(gString), "|- %s[%d]'s IP: %s -|",  PlayerName(target), target, ip);
		SendClientMessageEx(playerid, COLOR_GREEN, gString);
		format(gString, sizeof(gString), "[*] Admin %s[%d] | Level: Helper | Command: IP", PlayerName(playerid), playerid);
	}
	else return SendClientMessageEx(playerid, COLOR_RED, "[ERROR] You do not have permission to use this command!");
	return 1;
}

CMD:announce(playerid, params[])
{
	if(PlayerInfo[playerid][pAdmin] < 3) return SendClientMessageEx(playerid, COLOR_ORANGE, "[*] You're not a Level 3+ admin!");
