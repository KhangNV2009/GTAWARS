CMD:announce2(playerid, params[])
{

    if(PlayerInfo[playerid][pAdmin] < 3) return SendClientMessageEx(playerid, COLOR_ORANGE, "[*] You're not a Level 3+ admin!");
    new text[64], time, style;
    if(sscanf(params, "iis[64]", style, time, text)) return SendClientMessageEx(playerid, COLOR_RED, "Usage: /announce2 <style[0-6]> <time in ms> <text>");
    if(strlen(text) > 64) return SendClientMessageEx(playerid, COLOR_RED, "Message too long please shorten it ");
