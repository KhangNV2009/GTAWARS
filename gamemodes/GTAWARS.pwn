public OnPlayerDisconnect(playerid, reason)
{
	foreach (Zones, i) if (CapturingZone[playerid] == i) LeavingCAPZONE(playerid, i);
	PlayerInfo1[playerid][DM] = 0;
 ClassTaken[playerid] = 0;
	PlayerInfo[playerid][LastDeath] = 0;
	PlayerInfo[playerid][pMuted] = 0;
	PlayerInfo[playerid][DeathSpam] = 0;
	FirstSpawn[playerid] = 0;

	MAX_PLAYERS_ = GetHighestID(playerid);
	if(var[playerid] != -1)
	{
		KillTimer(var[playerid]);
		var[playerid] = -1;
	}
	warns[playerid] = 0;

	if(PlayerInfo[playerid][spawnedCar] == 1)
	{
		DestroyVehicle(PlayerInfo[playerid][spawnedCar]);
	}

	new string[100 + MAX_PLAYER_NAME];
