		SetPlayerPos(playerid, slx, sly, slz + 2);
	}
	if(GetVehicleModel(vehicleid) == 447 && gClass[playerid] != Pilot)//if player is pilot then he can drive
	{
		SendClientMessageEx(playerid, COLOR_RED, "(ERROR): Ban phai la pilot de lai seasparrow.");//messages goes to the player that he can't drive the hunter
  		RemovePlayerFromVehicle(playerid);//get u off from the hunter
        new Float:slx, Float:sly, Float:slz;
		GetPlayerPos(playerid, slx, sly, slz);
		SetPlayerPos(playerid, slx, sly, slz + 2);
	}
	if(GetVehicleModel(vehicleid) == 425 && gClass[playerid] != Pilot)//if player is pilot then he can drive
	{
		SendClientMessageEx(playerid, COLOR_RED, "(ERROR): Ban phai la pilot de lai hunter.");//messages goes to the player that he can't drive the hunter
		RemovePlayerFromVehicle(playerid);//get u off from the hunter
 		new Float:slx, Float:sly, Float:slz;
		GetPlayerPos(playerid, slx, sly, slz);
		SetPlayerPos(playerid, slx, sly, slz + 2);
	}
	else if(GetVehicleModel(vehicleid) == 425 && GetPlayerScore(playerid) < 2501)//if player is pilot then he can drive
	{
		SendClientMessageEx(playerid, COLOR_RED, "Ban phai la Colonel (2500 scores) de lai hunter");
		RemovePlayerFromVehicle(playerid);//get u off from the hunter
 		new Float:slx, Float:sly, Float:slz;
		GetPlayerPos(playerid, slx, sly, slz);
		SetPlayerPos(playerid, slx, sly, slz + 2);
	}
	if(GetVehicleModel(vehicleid) == 520 && gClass[playerid] != Pilot)//if player is pilot then he can drive
	{
		SendClientMessageEx(playerid, COLOR_RED, "(ERROR): Ban phai la pilot de lai hydra.");//messages goes to the player that he can't drive the hunter
		RemovePlayerFromVehicle(playerid);//get u off from the hunter
 		new Float:slx, Float:sly, Float:slz;
		GetPlayerPos(playerid, slx, sly, slz);
		SetPlayerPos(playerid, slx, sly, slz + 2);
	}
	else if(GetVehicleModel(vehicleid) == 520 && GetPlayerScore(playerid) < 1501)//if player is pilot then he can drive
	{
		SendClientMessageEx(playerid, COLOR_RED, "Ban phai la Major (1500 scores) de lai hunter");
		RemovePlayerFromVehicle(playerid);//get u off from the hunter
 		new Float:slx, Float:sly, Float:slz;
		GetPlayerPos(playerid, slx, sly, slz);
		SetPlayerPos(playerid, slx, sly, slz + 2);
	}
	if(newstate == PLAYER_STATE_DRIVER)
	{
	    DayThungSEAT[playerid] = 0;
		SetPlayerArmedWeapon(playerid, 0);
 	}


	if(newstate == PLAYER_STATE_PASSENGER)
	{
	    DayThungSEAT[playerid] = 1;
		if(!IsVehicleHasDriver(vehicleid))
		{
			new Float:vx, Float:vy, Float:vz;
			GetVehiclePos(vehicleid, vx, vy, vz);
			SetPlayerPos(playerid, vx, vy, vz+5);
			SetTimerEx("Fix", 500, false, "ii", playerid, vehicleid);
			SendClientMessageEx(playerid, 0xFF0000FF, "Khong duoc vao 1 phuong tien khi khong co tai xe!");
			printf("[NSAPA]: nguoi choi %i da vao xe %i khi khong co tai xe.", playerid, vehicleid);
			return 1;
		}
	}
	if(newstate == PLAYER_STATE_DRIVER) return SetPVarInt(playerid, "vehicleid", GetPlayerVehicleID(playerid));
	if(newstate == PLAYER_STATE_ONFOOT && oldstate == PLAYER_STATE_DRIVER)
	{
		for(new passengerid = 0; passengerid < MAX_PLAYERS; passengerid++)
		{
			if(IsPlayerConnected(passengerid) && GetPlayerVehicleID(passengerid) == GetPVarInt(playerid, "vehicleid"))
			{
				RemovePlayerFromVehicle(passengerid);
				SendClientMessageEx(passengerid, 0xFF0000FF, "Nguoi lai xe dang roi khoi phuong tien");
				printf("[NSAPA][APAADL]: Nguoi choi %i Da bi kick khoi phuong tien %i (Nguoi choi %i da bi kick khoi phuong tien).", passengerid, GetPVarInt(playerid, "vehicleid"), playerid);
			}
		}
		SetPVarInt(playerid, "vehicleid", 0);
		return 1;
	}

	// Vehicle hack detection
	if(USE_ANTI_VEHICLE_HACK == true)
	{
		if(IsPlayerInAnyVehicle(playerid) || newstate == PLAYER_STATE_DRIVER || newstate == PLAYER_STATE_PASSENGER)
		{
			pVehicles[playerid]++;
			SetTimerEx("VehicleEnterReset", 3000, 0, "i", playerid);
			if(pVehicles[playerid] >= MAX_ENTER_VEHICLES)
			{
				SendClientMessageEx(playerid, COLOR_RED, "ANTI-CHEAT: [VEHICLE HACKS] - Ban da bi kick vi co the duoc cho la hack phuong tien.");
				SetTimerEx("UnsetKick", 500, 0, "i", playerid);
			}
		}
	}

	for(new x=0; x<MAX_PLAYERS; x++)
	{
		if(GetPlayerState(x) == PLAYER_STATE_SPECTATING && PlayerInfo[x][SpectateID] == playerid && PlayerInfo[x][SpectateType] == ADMIN_SPEC_TYPE_VEHICLE)
		{
			if(GetPlayerState(playerid) == PLAYER_STATE_NONE)
			{
				StopSpectate(x);
			}
		}
	}

	for(new x=0; x<MAX_PLAYERS; x++)
	{
		if(GetPlayerState(x) == PLAYER_STATE_SPECTATING && PlayerInfo[x][SpectateID] == playerid && PlayerInfo[x][SpectateType] == ADMIN_SPEC_TYPE_VEHICLE)
		{
			if(GetPlayerState(playerid) == PLAYER_STATE_ONFOOT)
			{
				TogglePlayerSpectating(x, 1);
				PlayerSpectatePlayer(x, playerid);
				PlayerInfo[x][SpectateType] = ADMIN_SPEC_TYPE_PLAYER;
			}
		}
	}

	for(new x=0; x<MAX_PLAYERS; x++)
	{
		if(GetPlayerState(x) == PLAYER_STATE_SPECTATING && PlayerInfo[x][SpectateID] == playerid)
		{
			if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER || GetPlayerState(playerid) == PLAYER_STATE_PASSENGER)
			{
				TogglePlayerSpectating(x, 1);
				PlayerSpectateVehicle(x, vehicleid);
				PlayerInfo[x][SpectateType] = ADMIN_SPEC_TYPE_VEHICLE;
			}
		}
	}

	for(new x=0; x<MAX_PLAYERS; x++)
	{
		if(GetPlayerState(x) == PLAYER_STATE_SPECTATING && PlayerInfo[x][SpectateID] == playerid)
		{
			if(GetPlayerState(playerid) == PLAYER_STATE_WASTED)
			{
				AdvanceSpectate(x);
			}
		}
	}
	return 1;
}

