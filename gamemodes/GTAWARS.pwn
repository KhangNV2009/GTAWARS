forward CheckEmail(str[],size);
public CheckEmail(str[],size)
{
	new fp[32],sp[32];
	if(sscanf(str,"p<@>s[32]s[32]",fp,sp)) return 0;
	new bool:dg = false;
	for(new i = 0; i < size; i++)
	{
	    if('a' <= str[i] <= 'z' || 'A' <= str[i] <= 'Z' || '0' <= str[i] <= '9' ||  str[i] == '-' || str[i] == '.' || str[i] == '_') continue;
	    else
		{
		    if(str[i] == '@')
		    {
		        if(!dg)
		        {
		            dg = true;
		            continue;
				}
				else return 0;
			}
			else return 0;
		}
	}
	return 1;
}

