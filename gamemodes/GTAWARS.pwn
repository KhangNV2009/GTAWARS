    return 1;
}

FindIP(playerid,text[])
{
	new is1 = 0;
 	new r = 0;
 	new strR[100];
 	while(strlen(text[is1]))
 	{
  		if('0' <= text[is1] <= '9')
	  	{
		 	new is2 = is1+1;
		  	new p = 0;
		   	while(p == 0)
		  	{
			   	if('0' <= text[is2] <= '9' && strlen(text[is2])) is2++;
			 	else
			  	{
				   	strmid(strR[r],text,is1,is2,255);
				   	if(strval(strR[r]) < 255) r++;
				    is1 = is2;
				    p = 1;
			    }
		    }
	    }
	    is1++;
 	}
 	if(r >= 4)
  	{
  	    new pname[MAX_PLAYER_NAME];
  	    GetPlayerName(playerid,pname,sizeof(pname));
        SendClientMessageEx(playerid,COLOR_RED,"[WARNING]: Khong duoc quang cao server khac , 1 lan nua se bi ban!");
