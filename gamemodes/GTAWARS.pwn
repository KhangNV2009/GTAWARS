CMD:heal(playerid, params[])
{
    new pID;
    if(sscanf(params, "u", pID)) return SendClientMessageEx(playerid, COLOR_RED, "Usage: /heal playerid");
    if(gClass[playerid] == Medic || gClass[playerid] == Supporter)
    {
    if(HealthCD[playerid] == 1) return SendClientMessageEx(playerid, COLOR_RED, "Please wait another 1 minute before using /heal again");
    if(IsPlayerInRangeOfPlayer(playerid, pID, 5))
    {
        if(playerid != pID)
        {
            SetPlayerHealth(pID, 100);
            GivePlayerMoney(playerid, 150);
            GivePlayerMoney(pID, -150);
            HealthCD[playerid] = 1;
            SetTimerEx("EndHealthAbuse", 60000, false, "i", playerid);
            SendClientMessageEx(pID, COLOR_RED, "You have been healed.");
            SendClientMessageEx(playerid, COLOR_RED, "You have healed the player.");
        }
        else
        {
            SendClientMessageEx(playerid, COLOR_RED, "You can't heal yourself.");
        }
    }
    else
    {
        SendClientMessageEx(playerid, COLOR_RED, "You aren't close enough to the player.");
    }
    }
    else SendClientMessageEx(playerid, COLOR_RED, "(ERROR): You should be in medic class, to use this command");
    return 1;
}

