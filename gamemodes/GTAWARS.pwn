CMD:eget(playerid, params[])
{
	if(PlayerInfo[playerid][pAdmin] > 1)
	{
	new target;
	if(sscanf(params, "u", target)) return SendClientMessageEx(playerid, COLOR_ORANGE, "[*] Usage: /get(here) [playerid/name]");
	else if(target == INVALID_PLAYER_ID) return SendClientMessageEx(playerid, COLOR_ORANGE, "[*] Enter a valid player ID/name!");
	new Float:Pos[3];
	GetPlayerPos(playerid, Pos[0], Pos[1], Pos[2]);
	SetPlayerPos(target, Pos[0] + 2, Pos[1] + 2, Pos[2]);
	SetPlayerInterior(target, GetPlayerInterior(playerid));
	TogglePlayerControllable(target, 0);
	ResetPlayerWeapons(target);
	format(gString, SOS, "[*] Admin %s[%d] | Level: %d | Command: eget | Target: %s", PlayerName(playerid), playerid, PlayerInfo[playerid][pAdmin], PlayerName(target));
    }
	else SendClientMessageEx(playerid, COLOR_ORANGE, "[*] You're not a Level 1+ admin!");
	return 1;
}

