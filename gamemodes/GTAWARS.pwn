public OnPlayerText(playerid, text[])
{

	if(ServerInfo[AntiAdv] == 1)
	{
		if(FindIP(playerid, text)) return 0;
		if(FindSite(playerid, text)) return 0;
	}
	if(PlayerInfo[playerid][pMuted] == 1 && PlayerInfo[playerid][pAdmin] == 0)
	{
		PlayerInfo[playerid][MutedWarnings]++;
		format(gString, SOS, "WARNING: Ban dang bi mute va khong the chat (Warning: %d/5)", PlayerInfo[playerid][MutedWarnings]);
		SendClientMessageEx(playerid, COLOR_RED, gString);
		if(PlayerInfo[playerid][MutedWarnings] == 5)
		{
			format(gString, SOS, "He thong da kick %s[%d] [Ly do: Spam]", PlayerName(playerid), playerid);
			SendClientMessageToAllEx(COLOR_RED, gString);
			Kick(playerid);
			format(gString, SOS, "~g~%s ~w~da bi  ~r~kick ~w~khoi serv?", PlayerName(playerid));
			SendBoxMessage(playerid, gString);
			return 1;
		}
		return 0;
	}
	if(USE_ANTI_SPAM == true)
	{
		if(PlayerInfo[playerid][pMuted] == 1)
		{
			SendClientMessageEx(playerid, COLOR_RED, "ERROR: You are muted for spamming.");
			return 0;
		}
		pLineCount[playerid]++;
		SetTimerEx("LineCountReset", 2000, 0, "i", playerid);
		if(pLineCount[playerid] >= MAX_MESSAGES)
		{
			PlayerInfo[playerid][pMuted] = 1;
			SetTimerEx("Unmute", 30000, 0, "i", playerid);
		}
	}
	switch(PlayerInfo[playerid][AdminDuty])
	{
		case 0:
		{
			new str[128];
			format(str, sizeof(str), "%s (%d): {FFFFFF}%s", PlayerName(playerid), playerid, text);
			SendClientMessageToAllEx(GetPlayerColor(playerid), str);
			return 0;
		}
		case 1:
		{
			new string[128];
			format(string, sizeof(string), "Admin %s: {FFFFFF}%s", PlayerName(playerid), text);
			SendClientMessageToAllEx(0xFD01FDAA, string);
			return 0;
		}
	}
	return 1;
}

