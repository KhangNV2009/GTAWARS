forward ZoneTimer(playerid);
public ZoneTimer(playerid)
{
	foreach (Zones, i) if (UnderAttack[i] && zonetimer[i])
	{
		zonetimer[i]--;
		foreach (Player, j)
		{
		    if (CapturingZone[j] == i)
		    {
		        SetPlayerProgressBarValue(j, capturebar[j], 25 - zonetimer[i]);
		        UpdatePlayerProgressBar(j, capturebar[j]);
		    }
		}
		if (!zonetimer[i]) CAPZONECaptured(i);
	}
	return 1;
}


forward OnPlayerEnterDynamicCP(playerid, checkpointid);
public OnPlayerEnterDynamicCP(playerid, checkpointid)
{
    foreach (Zones, i) if (checkpointid == CP[i])
    {
		if(gTeam[playerid] == Guerilla) return SendClientMessageEx(playerid, COLOR_RED, "*Team Guerilla khong the chiem dong, Dung /st neu muon doi team");
	    if (checkpointid != CP[i]) continue;
        if (tCP[i] == gTeam[playerid]) return SendClientMessageEx(playerid, COLOR_RED,"*Dia diem nay da duoc chiem boi team cua ban!");
        ActiveCAPZONE(playerid, i);
	}
    return 1;
}

forward OnPlayerLeaveDynamicCP(playerid, checkpointid);
public OnPlayerLeaveDynamicCP(playerid, checkpointid)
{
	foreach (Zones, i)
	{
	    if (checkpointid != CP[i]) continue;
	    if (CapturingZone[playerid] == i && !IsPlayerInDynamicCP(playerid, CP[i])) LeavingCAPZONE(playerid, i);
    }
    return 1;
}

stock ActiveCAPZONE(playerid, zoneid)
{
    if (IsPlayerInAnyVehicle(playerid)) return SendClientMessageEx(playerid, COLOR_RED,"Khong the chiem khi dang o tren 1 phuong tien!");
	if (GetPlayerState(playerid) == PLAYER_STATE_SPECTATING) return SendClientMessageEx(playerid, COLOR_RED, "Khong the chiem khi dang trong spec mode!");
	new cnt = GetCapturersCount(zoneid);
 	if(gTeam[playerid] == USA) GangZoneFlashForAll(Zone[zoneid], USA_GZ_COLOR);
	else if(gTeam[playerid] == UK) GangZoneFlashForAll(Zone[zoneid], UK_GZ_COLOR);
	else if(gTeam[playerid] == RUS) GangZoneFlashForAll(Zone[zoneid], RUS_GZ_COLOR);
	else if(gTeam[playerid] == ARAB) GangZoneFlashForAll(Zone[zoneid], ARAB_GZ_COLOR);
	else if(gTeam[playerid] == ASIAN) GangZoneFlashForAll(Zone[zoneid], ASIAN_GZ_COLOR);
	format(gString, SOS, "*%s Dang bi tan cong , hay den do va chiem dia diem do!", ZoneInfo[zoneid][namee]);
    SendClientMessageToAllEx(GREEN, gString);
	switch (cnt)
	{
	    case 0:
	    {
	        UnderAttack[zoneid] = true;
		    CapturingZone[playerid] = zoneid;
		    capturingteam[zoneid] = gTeam[playerid];
		    zonetimer[zoneid] = 25;
		    SendClientMessageEx(playerid, COLOR_WHITE,"O trong vong tron do 25 giay de chiem! Ban co the chiem nhanh hon khi co dong doi cung chiem.");
		    ShowPlayerProgressBar(playerid, capturebar[playerid]);
		    SetPlayerProgressBarValue(playerid, capturebar[playerid], 0);
		    UpdatePlayerProgressBar(playerid, capturebar[playerid]);
		    TextDrawShowForPlayer(playerid, CaptureZoneTextDraw[0]);
		    TextDrawShowForPlayer(playerid, CaptureZoneTextDraw[1]);
		    TextDrawShowForPlayer(playerid, CaptureZoneTextDraw[2]);

			if (tCP[zoneid] == USA)
			{
   				TextDrawShowForPlayer(playerid, CaptureZoneTextDraw[3]);
			}
			if (tCP[zoneid] == UK)
			{
   				TextDrawShowForPlayer(playerid, CaptureZoneTextDraw[4]);
			}
			if (tCP[zoneid] == RUS)
			{
   				TextDrawShowForPlayer(playerid, CaptureZoneTextDraw[5]);
			}
			if (tCP[zoneid] == ARAB)
			{
   				TextDrawShowForPlayer(playerid, CaptureZoneTextDraw[6]);
			}
			if (tCP[zoneid] == ASIAN)
			{
   				TextDrawShowForPlayer(playerid, CaptureZoneTextDraw[7]);
			}
		}
	    case 1 .. 4:
	    {
            new string[128];
			format(string, sizeof(string), "Nguoi choi %s dang giup chiem dong dia diem!", GetName(playerid));
			foreach (Player, i)
			{
			if (i == playerid) continue;
			if (CapturingZone[i] != zoneid) continue;
			if (gTeam[i] == capturingteam[zoneid]) continue;
			SendClientMessageEx(i, -1, string);
			}
	        if (capturingteam[zoneid] != gTeam[playerid]) return SendClientMessageEx(playerid, COLOR_RED, "Nguoi chiem khong cung team voi ban!");
            CapturingZone[playerid] = zoneid;
			SendClientMessageEx(playerid, COLOR_WHITE, "Ban da tham gia chiem dong dia diem.");
			ShowPlayerProgressBar(playerid, capturebar[playerid]);
			SetPlayerProgressBarValue(playerid, capturebar[playerid], zonetimer[zoneid]);
	        UpdatePlayerProgressBar(playerid, capturebar[playerid]);
	        TextDrawShowForPlayer(playerid, CaptureZoneTextDraw[0]);
		    TextDrawShowForPlayer(playerid, CaptureZoneTextDraw[1]);
   			TextDrawShowForPlayer(playerid, CaptureZoneTextDraw[2]);

			if (tCP[zoneid] == USA)
			{
   				TextDrawShowForPlayer(playerid, CaptureZoneTextDraw[3]);
			}
			if (tCP[zoneid] == UK)
			{
   				TextDrawShowForPlayer(playerid, CaptureZoneTextDraw[4]);
			}
			if (tCP[zoneid] == RUS)
			{
   				TextDrawShowForPlayer(playerid, CaptureZoneTextDraw[5]);
			}
			if (tCP[zoneid] == ARAB)
			{
   				TextDrawShowForPlayer(playerid, CaptureZoneTextDraw[6]);
			}
			if (tCP[zoneid] == ASIAN)
			{
   				TextDrawShowForPlayer(playerid, CaptureZoneTextDraw[7]);
			}
			switch (cnt)
	        {
	            case 1: zonetimer[zoneid] = zonetimer[zoneid] / 2;
	            case 2: zonetimer[zoneid] = (zonetimer[zoneid] / 3) * 2;
	            case 3: zonetimer[zoneid] = (zonetimer[zoneid] / 4) * 3;
	            case 4: zonetimer[zoneid] = (zonetimer[zoneid] / 5) * 4;
	        }
	    }
		default: return SendClientMessageEx(playerid, COLOR_RED, "5 nguoi choi dang chiem dia diem nay , khong the chiem qua 5 nguoi cung 1 dia diem!");
	}
    return 1;
}

stock CAPZONECaptured(zoneid)
{
	UnderAttack[zoneid] = false;
	zonetimer[zoneid] = 0;
	tCP[zoneid] = capturingteam[zoneid];
	GangZoneStopFlashForAll(Zone[zoneid]);
	switch (capturingteam[zoneid])
	{
		case USA: GangZoneShowForAll(Zone[zoneid], USA_GZ_COLOR);
		case UK: GangZoneShowForAll(Zone[zoneid], UK_GZ_COLOR);
		case RUS: GangZoneShowForAll(Zone[zoneid], RUS_GZ_COLOR);
		case ARAB: GangZoneShowForAll(Zone[zoneid], ARAB_GZ_COLOR);
		case ASIAN: GangZoneShowForAll(Zone[zoneid], ASIAN_GZ_COLOR);
	}

	//gTeam[playerid] = CapturingZone[i];
	format(gString, SOS, "~y~[ZONE]~g~%s ~w~Da Bi Chiem !", ZoneInfo[zoneid][namee]);
	SendBoxMessage(-1, gString);
	foreach (Player, i)
	{
		if (gTeam[i] == capturingteam[zoneid])
		{
		    new zcCash = random(5000);
			format(gString, sizeof(gString), "%s Da Bi Chiem - Nhan Duoc $%d va 1 Score!", ZoneInfo[zoneid][namee], zcCash);
			SendClientMessageEx(i, COLOR_WHITE, gString);
			GivePlayerMoney(i, zcCash);
			GivePlayerScores(i, 1);
		}
		else if (gTeam[i] != capturingteam[zoneid])
		{
		    new zCash = random(3000);
			format(gString, sizeof(gString), "%s Da Bi Chiem - Bi Tru -$%d!", ZoneInfo[zoneid][namee], zCash);
			SendClientMessageEx(i, COLOR_WHITE, gString);
			GivePlayerMoney(i, -zCash);
		}
		if (CapturingZone[i] == zoneid)
		{
			SendClientMessageEx(i, COLOR_GREEN, "Chuc mung ! Ban da chiem thanh cong dia diem! Ban nhan duoc 5 Scores va 5000 tien!");
			GivePlayerScores(i, 5);
			GivePlayerMoney(i, 5000);
			CapturingZone[i] = -1;
			HidePlayerProgressBar(i, capturebar[i]);
			TextDrawHideForPlayer(i, CaptureZoneTextDraw[0]);
			TextDrawHideForPlayer(i, CaptureZoneTextDraw[1]);
			TextDrawHideForPlayer(i, CaptureZoneTextDraw[2]);
			TextDrawHideForPlayer(i, CaptureZoneTextDraw[3]);
			TextDrawHideForPlayer(i, CaptureZoneTextDraw[4]);
			TextDrawHideForPlayer(i, CaptureZoneTextDraw[5]);
			TextDrawHideForPlayer(i, CaptureZoneTextDraw[6]);
			TextDrawHideForPlayer(i, CaptureZoneTextDraw[7]);
		}
	}
	return 1;
}

stock LeavingCAPZONE(playerid, zoneid)
{
    CapturingZone[playerid] = -1;
	new cnt = GetCapturersCount(zoneid);
	switch (cnt)
	{
	    case 0:
	    {
	        UnderAttack[zoneid] = false;
		    GangZoneStopFlashForAll(Zone[zoneid]);
		    SendClientMessageEx(playerid, COLOR_RED,"*Ban da roi khoi vung chiem dong, chiem dong that bai!");
		    zonetimer[zoneid] = 0;
	    }
	    case 1 .. 4:
		{
			SendClientMessageEx(playerid, COLOR_RED, "Ban da ra khoi vong tron` do , khong the chiem dong!");
			switch (cnt)
	        {
	            case 1: zonetimer[zoneid] = zonetimer[zoneid] * 2;
	            case 2: zonetimer[zoneid] = (zonetimer[zoneid] * 3) / 2;
	            case 3: zonetimer[zoneid] = (zonetimer[zoneid] * 4) / 3;
	            case 4: zonetimer[zoneid] = (zonetimer[zoneid] * 5) / 4;
	        }
		}
	}
	CapturingZone[playerid] = -1;
	HidePlayerProgressBar(playerid, capturebar[playerid]);
	TextDrawHideForPlayer(playerid, CaptureZoneTextDraw[0]);
	TextDrawHideForPlayer(playerid, CaptureZoneTextDraw[1]);
	TextDrawHideForPlayer(playerid, CaptureZoneTextDraw[2]);
	TextDrawHideForPlayer(playerid, CaptureZoneTextDraw[3]);
	TextDrawHideForPlayer(playerid, CaptureZoneTextDraw[4]);
	TextDrawHideForPlayer(playerid, CaptureZoneTextDraw[5]);
    return 1;
}

