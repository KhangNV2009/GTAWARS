forward UpdateLaunchTime();
public UpdateLaunchTime()
{
	if(nuke_time>=1)
	{
	    if(nuke_time<=0) return nuke_time=0;
	    nuke_time--;
	}
	/*---------ATR---------*/
	if(ATRUSA_TIME>=1)//USA
	{
	    if(ATRUSA_TIME<=0) return ATRUSA_TIME=0;
	    ATRUSA_TIME--;
	}
	if(ATRUK_TIME>=1)
	{
	    if(ATRUK_TIME<=0) return ATRUK_TIME=0;
	    ATRUK_TIME--;
	}
	if(ATRRUS_TIME>=1)
	{
	    if(ATRRUS_TIME<=0) return ATRRUS_TIME=0;
	    ATRRUS_TIME--;
	}
	if(ATRARAB_TIME>=1)
	{
	    if(ATRARAB_TIME<=0) return ATRARAB_TIME=0;
	    ATRARAB_TIME--;
	}
	if(ATRASIAN_TIME>=1)
	{
	    if(ATRASIAN_TIME<=0) return ATRASIAN_TIME=0;
	    ATRASIAN_TIME--;
	}
	if(ATRASIAN_TIME>=1)
	{
	    if(ATRASIAN_TIME<=0) return ATRASIAN_TIME=0;
	    ATRASIAN_TIME--;
	}
	/*---------SAM---------*/
	if(SAM_TIME == 4)
	{
	    foreach(Player, i)
    	{
            if(PlayerInfo[i][pSammer] == 1)
			{
			    PlayerInfo[i][pSammer] = 0;
			}
		}
	}
	if(SAM_TIME>=1)
	{
	    if(SAM_TIME<=0) return SAM_TIME=0;
	    SAM_TIME--;
	}
	return 1;
}

stock IsPlayerInArea(playerid, Float:MinX, Float:MinY, Float:MaxX, Float:MaxY)
{
    new Float:X, Float:Y, Float:Z;

    GetPlayerPos(playerid, X, Y, Z);
    if(X >= MinX && X <= MaxX && Y >= MinY && Y <= MaxY) {
        return 1;
    }
    return 0;
}

