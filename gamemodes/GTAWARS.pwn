stock CountIP(ip[])
{
        new c = 0;
        for(new i = 0; i < MAX_PLAYERS; i++) if(IsPlayerConnected(i) && !strcmp(GetIP(i),ip)) c++;
        return c;
}

stock GetHighestID(exceptof = INVALID_PLAYER_ID)
{
        new h = 0;
        for(new i = 0; i < MAX_PLAYERS; i++) if(IsPlayerConnected(i) && i != exceptof && i > h) h = i;
        return h;
}

