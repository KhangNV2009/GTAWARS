CMD:v(playerid, params[])
{
	if(PlayerInfo[playerid][pAdmin] < 2) return SendClientMessageEx(playerid, COLOR_ORANGE, "[*] You aren't a Level 3+ admin!");
	new veh[24], color[2];
	if(sscanf(params, "s[24]I(-1)I(-1)", veh, color[0], color[1])) return SendClientMessageEx(playerid, COLOR_ORANGE, "[*] Usage: /car [vehicleid/name] [Optional: color 1] [Optional: color 2]");
    if(PlayerInfo[playerid][spawnedCar] != 0) DestroyVehicle(PlayerInfo[playerid][spawnedCar]);
	for(new v; v < 212; v++) if(strfind(VehicleNames[v], veh, true) != -1) { vehicle = v + 400; break; }
	if(vehicle == -1) vehicle = strval(veh);
	if(vehicle < 400 || vehicle > 611) return SendClientMessageEx(playerid, COLOR_ORANGE, "[*] Enter a valid vehicle name or an ID between 400 and 611!");
	new Float:Pos[4];
	if(IsPlayerInAnyVehicle(playerid))
	{
	    RemovePlayerFromVehicle(playerid);
	    GetPlayerPos(playerid, Pos[0], Pos[1], Pos[2]);
	    GetPlayerFacingAngle(playerid, Pos[3]);
	}
	else
	{
	    GetPlayerPos(playerid, Pos[0], Pos[1], Pos[2]);
	    GetPlayerFacingAngle(playerid, Pos[3]);
	}
	PlayerInfo[playerid][spawnedCar] = CreateVehicle(vehicle, Pos[0], Pos[1], Pos[2], Pos[3], color[0], color[1], CAR_RESPAWN_DELAY);
    SetVehicleToRespawn(PlayerInfo[playerid][spawnedCar]);
	PutPlayerInVehicle( playerid , PlayerInfo[playerid][spawnedCar] , 0);
	SetVehicleVirtualWorld(PlayerInfo[playerid][spawnedCar], GetPlayerVirtualWorld(playerid));
    LinkVehicleToInterior(PlayerInfo[playerid][spawnedCar], GetPlayerInterior(playerid));
	format(gString, SOS, "[*] Admin %s[%d] | Level: %d | Command: car", PlayerName(playerid), playerid, PlayerInfo[playerid][pAdmin]);

	return 1;
}


CMD:get(playerid, params[])
{
	if(PlayerInfo[playerid][pAdmin] < 1) return SendClientMessageEx(playerid, COLOR_ORANGE, "[*] You're not a Level 1+ admin!");
