public OnPlayerDeath(playerid, killerid, reason)
{
    if (killerid != INVALID_PLAYER_ID) // Valid killer, give cash+Scores
	{
		AntiFakekill[playerid]++;
		SetTimerEx("AntiFakekillTimer", 1000, false, "i", playerid);

		SendDeathMessage(killerid, playerid, reason);
		PlayerInfo[playerid][pDeaths] ++;
		PlayerInfo[killerid][pKills] ++;
		new Sstring[180];
		new ammo = GetPlayerAmmo(killerid);
		new weapon = GetPlayerWeapon(killerid);
		switch(weapon)
		{
			case 1 .. 15:
			{
				SetPlayerAmmo(killerid, weapon, ammo+1);
			}

			case 16 .. 18:
			{
				SetPlayerAmmo(killerid, weapon, ammo+1);
			}

			case 22 .. 34:
			{
				SetPlayerAmmo(killerid, weapon, ammo+25);
			}

			case 35 .. 47:
			{
				SetPlayerAmmo(killerid, weapon, ammo+1);
			}

