    if(style < 0 || style > 6) return SendClientMessageEx(playerid, COLOR_RED, "Invalid style");
    if(time > 20*1000) return SendClientMessageEx(playerid, COLOR_RED, "No longer than 20 seconds");
	format(gString, SOS, "[*] Admin %s[%d] | Level: %d | Command: Announce2", PlayerName(playerid), playerid, PlayerInfo[playerid][pAdmin]);
	GameTextForAll(text, time, style);

    return 1;
}

