	if(sscanf(params, "s[64]", text)) return SendClientMessageEx(playerid, COLOR_RED, "Usage: /announce <text>");
 	if(isnull(text)) return SendClientMessageEx(playerid, COLOR_ORANGE, "[*] Usage: /an(nounce) [message]");
	format(gString, SOS, "[*] Admin %s[%d] | Level: %d | Command: Announce", PlayerName(playerid), playerid, PlayerInfo[playerid][pAdmin]);
	GameTextForAll(text, 3800, 3);
	return 1;
}
