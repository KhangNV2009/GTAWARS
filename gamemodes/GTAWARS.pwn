CMD:givegun(playerid,params[])
{
    if(Sended[playerid] != 0)
    {
        new msg[128];
        format(msg, 128, "ERROR: Bna chi co the cho sung cach nhau %d giay!", WAIT_TIME);
        return SendClientMessageEx(playerid, COLOR_RED, msg);
    }
    new string[156], tmp[256], tmp2[256], weaponname[50], Index; tmp = strtok(params,Index); tmp2 = strtok(params,Index);
    if(!strlen(tmp) || !strlen(tmp2)) return SendClientMessageEx(playerid,COLOR_RED,"USAGE: /givegun [PlayerID] [Ammo]");
    new giveplayerid, ammo, weapon, weapon1, slot; giveplayerid=strval(tmp); ammo=strval(tmp2);
    if(!IsPlayerConnected(giveplayerid)) return SendClientMessageEx(playerid,COLOR_RED,"ERROR: nguoi choi khong ton tai!");
    if(playerid==giveplayerid) return SendClientMessageEx(playerid,COLOR_RED,"ERROR: Ban khong the tu cho vu khi cho chinh minh!");
    if(ammo<=0) return SendClientMessageEx(playerid,COLOR_RED,"ERROR: So bang dang khong hop le!");
    if(!IsPlayerNearPlayer(playerid, giveplayerid, 10.0)) return SendClientMessageEx(playerid, COLOR_RED, "Ban phai o gan nguoi choi ban muon cho sung!");
    weapon=GetPlayerWeapon(playerid); weapon1=GetPlayerWeapon(giveplayerid);
    if(weapon==0) return SendClientMessageEx(playerid,COLOR_RED,"ERROR: Ban khong co vu khi !");
    for(new i=0; i<sizeof(ForbiddenWeapons); i++) if(weapon==ForbiddenWeapons[i]) return SendClientMessageEx(playerid,COLOR_RED,"ERROR: Khong the cho vu khi nay!");
    SaveWeapons(playerid); SaveWeapons(giveplayerid);
    slot=GetWeaponSlot(weapon);
    if(savedammo[playerid][slot]<ammo) return SendClientMessageEx(playerid,COLOR_RED,"ERROR: Luong dan khong hop le!");
    savedammo[playerid][slot]-=ammo;
    ResetPlayerWeapons(playerid); ResetPlayerWeapons(giveplayerid);
    GetWeaponName(weapon, weaponname, 50);
    format(string, 156, "[Weapon] %s (ID:%d) Da cho ban sung %s (ID:%d) Voi so dan %d vien.", PlayerName(playerid), playerid, weaponname, weapon, ammo);
    SendClientMessageEx(giveplayerid, green, string);
    format(string, 156, "[Weapon] Ban da cho sung nguoi choi %s (ID:%d) vu khi  %s (ID:%d) voi so dan %d vien.", PlayerName(giveplayerid), giveplayerid, weaponname, weapon, ammo);
    SendClientMessageEx(playerid, green, string);
    GiveWeapons(playerid); GiveWeapons(giveplayerid);
    GivePlayerWeapon(giveplayerid, weapon, ammo);
    GivePlayerWeapon(giveplayerid, weapon1, 0);
    GivePlayerWeapon(playerid, weapon, 0);
    Sender[giveplayerid]=playerid; ReceivedWeapon[giveplayerid][0]=weapon; ReceivedWeapon[giveplayerid][1]=ammo;
    if(Receiver[playerid]!=0) KillTimer(Receiver[playerid]);
    new time=WAIT_TIME;
    if(time!=0) Sended[playerid]=SetTimerEx("CanSendAgain", time*1000, 0, "d", playerid);
    return 1;
}

stock IsPlayerNearPlayer(playerid, playerid2, Float: radius)
{
    new Float: pPos[4];
    GetPlayerPos(playerid2, pPos[0], pPos[1], pPos[2]);
    if(IsPlayerInRangeOfPoint(playerid, radius, pPos[0], pPos[1], pPos[2])) return 1;
    return 0;
}

forward GiveWeapons(playerid);
public GiveWeapons(playerid)
{
    for(new i=0; i<13; i++) GivePlayerWeapon(playerid, savedweapon[playerid][i], savedammo[playerid][i]);
    return 1;
}

forward CanSendAgain(playerid);
public CanSendAgain(playerid)
{
    Sended[playerid]=0;
    return 1;
}

stock GetWeaponSlot(weapon)
{
    switch(weapon)
    {
        case 0,1: return 0;
        case 2..9: return 1;
        case 22..24: return 2;
        case 25..27: return 3;
        case 28,29,32: return 4;
        case 30,31: return 5;
        case 33,34: return 6;
        case 35..38: return 7;
        case 16..18,39: return 8;
        case 41..43: return 9;
        case 10..15: return 10;
        case 44..46: return 11;
        case 40: return 12;
        default: return -1;
    }
    return -1;
}


forward SaveWeapons(playerid);
public SaveWeapons(playerid)
{
    for(new i=0; i<13; i++)
    {
        savedweapon[playerid][i]=0; savedammo[playerid][i]=0;
    }
    for(new i=0; i<13; i++) GetPlayerWeaponData(playerid, i, savedweapon[playerid][i], savedammo[playerid][i]);
    return 1;
}


//strtok
strtok(const string[], &index)
{
    new length = strlen(string);
    while ((index < length) && (string[index] <= ' '))
    {
        index++;
    }

    new offset = index;
    new result[20];
    while ((index < length) && (string[index] > ' ') && ((index - offset) < (sizeof(result) - 1)))
    {
        result[index - offset] = string[index];
        index++;
    }
    result[index - offset] = EOS;
    return result;
}

