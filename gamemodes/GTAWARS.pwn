CMD:mute(playerid, params[])
{
	if(PlayerInfo[playerid][pAdmin] >= 1)
	{
		new target, reason[128], time;
		if(sscanf(params, "uds[128]", target, time, reason)) return SendClientMessageEx(playerid, COLOR_ORANGE, "[*] Usage: /mute [playerid/name] [minutes] [reason]");
		if(target == INVALID_PLAYER_ID) return SendClientMessageEx(playerid, COLOR_ORANGE, "[*] Enter a valid player ID/name!");
		if(target == playerid) return SendClientMessageEx(playerid, COLOR_RED, "You cannot mute yourself!");
		if(PlayerInfo[target][pAdmin] >= 1) return SendClientMessageEx(playerid, COLOR_RED, "You cannot mute this administrator!");
		PlayerInfo[target][pMuted] = 1;
		PlayerInfo[target][pMutedTime] = time;
		mutedtime = SetTimerEx("MutedTimes", 60*1000*time, false, "d", target);
		format(gString, SOS, "[*] Admin %s[%d] | Level: %d | Command: Mute | Target: %s", PlayerName(playerid), playerid, PlayerInfo[playerid][pAdmin], PlayerName(target));
		format(gString, SOS, "Admin %s[%d] has muted %s[%d] [Time: %d minute(s)] [Reason: %s]", PlayerName(playerid), playerid, PlayerName(target), target, time, reason);
		SendClientMessageToAllEx(COLOR_WHITE, gString);
	}
	else SendClientMessageEx(playerid, COLOR_ORANGE, "[*] You're not a Level 2+ admin!");
	return 1;
}

CMD:unmute(playerid, params[])
{
	if(PlayerInfo[playerid][pAdmin] >= 1)
	{
		new target;
		if(sscanf(params, "u", target)) return SendClientMessageEx(playerid, COLOR_ORANGE, "[*] Usage: /unmute [playerid/name]");
		else if(target == INVALID_PLAYER_ID) return SendClientMessageEx(playerid, COLOR_ORANGE, "[*] Enter a valid player ID/name!");
			PlayerInfo[target][pMuted] = 0;
		PlayerInfo[target][pMutedTime] = 0;
		pMutedd[target] = false;
		format(gString, SOS, "[*] Admin %s[%d] | Level: %d | Command: UnMute | Target: %s", PlayerName(playerid), playerid, PlayerInfo[playerid][pAdmin], PlayerName(target));
		format(gString, SOS, "Admin %s[%d] has unmuted %s[%d]", PlayerName(playerid), playerid, PlayerName(target), target);
		SendClientMessageEx(target, COLOR_WHITE, gString);
