CMD:sv( playerid, params[ ] )
{
	if(PlayerInfo[playerid][pAdmin] < 1) return SendClientMessageEx(playerid, COLOR_ORANGE, "[*] You're not a Level 1+ admin!");
    new vehID, num;

    if ( sscanf( params, "dd",  vehID, num))
        return SendClientMessageEx( playerid, COLOR_RED, "SYNTAX: /sv <vehicle ID> <0/1, where 0 == respawn, 1 == destroy)>" );

    if ( vehID == INVALID_VEHICLE_ID )
        return SendClientMessageEx( playerid, COLOR_RED, "ERROR: Invalid vehicle ID." );

	if(num == 1)
	{
	    DestroyVehicle(vehID);
	    SendClientMessageEx( playerid, green, "Vehicle has been respawned succesfully." );
	}
	if(num == 0)
	{
		SetVehicleToRespawn(vehID);
		SendClientMessageEx( playerid, green, "Vehicle has been respawned succesfully." );
	}
	format(gString, SOS, "[*] Admin %s[%d] | Level: %d | Command: sv", PlayerName(playerid), playerid, PlayerInfo[playerid][pAdmin]);
    return 1;
}

