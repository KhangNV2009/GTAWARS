CMD:spec(playerid, params[])
{
	if(PlayerInfo[playerid][pAdmin] >= 1)
	{
		new target;
	    if(sscanf(params, "u", target)) return SendClientMessageEx(playerid, COLOR_ORANGE, "[*] Usage: /spec [playerid/name]");
	    else if(target == INVALID_PLAYER_ID) return SendClientMessageEx(playerid, COLOR_ORANGE, "[*] Enter a valid player ID/name!");
		else if(target == playerid) return SendClientMessageEx(playerid, COLOR_ORANGE, "[*] You cannot spec yourself");
		if(GetPlayerState(target) != 1 && GetPlayerState(target) != 2 && GetPlayerState(target) != 3) return SendClientMessageEx(playerid, COLOR_RED, "Spectate: Player not spawned");
		new targeted, reporter;
		if(target == targeted)
		{
		     GameTextForPlayer(reporter, "~r~[]~w~An Admin Is Checking Your Report~r~[]", 3000, 3);
		}
	 	StartSpectate(playerid, target);
		SendClientMessageEx(playerid,COLOR_GREY,"Now Spectating");
	}
	else if(PlayerInfo[playerid][pHelper] >= 2)
	{
		new target;
	    if(sscanf(params, "u", target)) return SendClientMessageEx(playerid, COLOR_ORANGE, "[*] Usage: /spec [playerid/name]");
	    else if(target == INVALID_PLAYER_ID) return SendClientMessageEx(playerid, COLOR_ORANGE, "[*] Enter a valid player ID/name!");
		else if(target == playerid) return SendClientMessageEx(playerid, COLOR_ORANGE, "[*] You cannot spec yourself");
		else if(PlayerInfo[playerid][pAdmin] > PlayerInfo[playerid][pAdmin]) return SendClientMessageEx(playerid, COLOR_ORANGE, "[*] You cannot spectate this admin!");
		if(GetPlayerState(target) != 1 && GetPlayerState(target) != 2 && GetPlayerState(target) != 3) return SendClientMessageEx(playerid, COLOR_RED, "Spectate: Player not spawned");
	 	StartSpectate(playerid, target);
		SendClientMessageEx(playerid,COLOR_GREY,"Now Spectating");
	}
	else return SendClientMessageEx(playerid, COLOR_RED, "You do not have permission to use this command!");
	return 1;
}

stock StartSpectate(playerid, target)
{
	for(new x=0; x<MAX_PLAYERS; x++)
	{
	    if(GetPlayerState(x) == PLAYER_STATE_SPECTATING && PlayerInfo[x][SpectateID] == playerid)
		{
	       AdvanceSpectate(x);
		}
	}
	new ip[16], spec[512], Float:armour, Float:health;
	GetPlayerArmour(target, armour);
	GetPlayerHealth(target, health);
	GetPlayerIp(target, ip, sizeof(ip));
	TogglePlayerSpectating(playerid, 1);
	format(spec, sizeof(spec), "~n~~n~~n~~n~~n~~n~~w~%s ID:%d ~n~~r~$%d-%d ~n~~w~IP:%s~n~ Armour: %0.1f - Health: %0.1f", PlayerName(target), target, GetPlayerMoney(target), GetPlayerScore(target), ip, armour, health);
	GameTextForPlayer(playerid, spec, 15000, 3);
	PlayerInfo[playerid][SpectateUpdate] = target;
	SetPlayerInterior(playerid,GetPlayerInterior(target));
	SetPlayerVirtualWorld(playerid, GetPlayerVirtualWorld(target));
	if(IsPlayerInAnyVehicle(target))
	{
		PlayerSpectateVehicle(playerid, GetPlayerVehicleID(target));
		PlayerInfo[playerid][SpectateID] = target;
		PlayerInfo[playerid][SpectateType] = ADMIN_SPEC_TYPE_VEHICLE;
	}
	else
	{
		PlayerSpectatePlayer(playerid, target);
		PlayerInfo[playerid][SpectateID] = target;
		PlayerInfo[playerid][SpectateType] = ADMIN_SPEC_TYPE_PLAYER;
	}
	return 1;
}

stock AdvanceSpectate(playerid)
{
    if(ConnectedPlayers() == 2) { StopSpectate(playerid); return 1; }
	if(GetPlayerState(playerid) == PLAYER_STATE_SPECTATING && PlayerInfo[playerid][SpectateID] != INVALID_PLAYER_ID)
	{
	    for(new x=PlayerInfo[playerid][SpectateID]+1; x<=MAX_PLAYERS; x++)
		{
	    	if(x == MAX_PLAYERS) x = 0;
	        if(IsPlayerConnected(x) && x != playerid)
			{
				if(GetPlayerState(x) == PLAYER_STATE_SPECTATING && PlayerInfo[x][SpectateID] != INVALID_PLAYER_ID || (GetPlayerState(x) != 1 && GetPlayerState(x) != 2 && GetPlayerState(x) != 3))
				{
					continue;
				}
				else
				{
					StartSpectate(playerid, x);
					break;
				}
			}
		}
	}
	return 1;
}
forward ConnectedPlayers();
public ConnectedPlayers()
{
	new Connected;
	foreach(Player,i) if(IsPlayerConnected(i)) Connected++;
	return Connected;
}

